<@l.main>
<div class="well">
	<h1>Carga de comparendos</h1>

	<@l.form action=controllerStatics.PATH_CARGA_COMPARENDOS enctype='multipart/form-data'>

	<div class="form-group">
	<label for="datos">Archivo CSV de datos</label>
	<input type="file" id="datos" name="datos" />
	</div>

	<div class="form-group">
	<label for="imagen">Imagen del comparendo</label>
	<input type="file" id="imagen" name="imagen" />
	</div>

	<p>
		<@l.formSubmit label="Cargar ahora"/>
		<a href="<@s.url controllerStatics.PATH />" class="btn btn-link">Cancelar</a>
	</p>

	</@l.form>
</div>
</@l.main>