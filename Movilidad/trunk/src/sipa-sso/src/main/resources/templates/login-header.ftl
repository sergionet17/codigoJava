<#import "/spring.ftl" as spring/>
<#import "/layout.ftl" as layout/>

<@layout.main>
<div class="container-fluid">
<div class="row sipa-fondo-1">
<div class="col-md-2 sipa-fondo-1 sipa-p-0">
   <a href="<@spring.url '/'/>">
            <img id="sipa-logo" class="img-responsive"
            src="<@spring.url '/static/img/logo-alcaldia.png' />" >
          </a>
</div>
 <div class="col-md-10 sipa-p-0">
          <div id="sipa-top" class="sipa-fondo-1">
            <div>
              <h2 class="sipa-m-0">
              <@spring.message 'app.secretaria'/><br/>
              </h2>
              <h3 class="sipa-m-0">
              <@spring.message 'app.title'/> - <@spring.message 'app.title.descripcion'/>
              </h3>
            </div>
          </div>
        </div>  
</div>  
</div>
<br>
</@layout.main>