# A root 'au-cms-util-test-concurrency' folder will be created here to house the test documents...
cmis.folder.parent=mytestfolder

# How many simultaneous client users should be simulated by the test? 10
cmis.client.count=2

# How many documents should each client create/increment/delete during the test run? 20
cmis.documents.count=2

# How many times should a client increment the incrementParam+content for each document? 5
cmis.increments.count=2

# Number of folders to store the documents
cmis.subfolder.count=2
# Type name used for concurrency testing
cmis.type.name =mytesttype