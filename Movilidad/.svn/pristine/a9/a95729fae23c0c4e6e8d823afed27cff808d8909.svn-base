## This Alfresco Repo Configuration file should be used for custom properties that are introduced by this module.
## Define default values for all properties here.
## System Administrators can override these values in environment specific configurations in
## alfresco/tomcat/shared/classes/alfresco-global.properties.
##

# Custom Transformer configurations

# PDF -> TXT
content.transformer.pdf2ocr2txt.priority=30
content.transformer.pdf2ocr2txt.extensions.pdf.txt.supported=true
content.transformer.pdf2ocr2txt.extensions.pdf.txt.priority=30
content.transformer.pdf2ocr2txt.extensions.pdf.txt.maxSourceSizeKBytes.use.index=99999

# TIFF -> TXT (Indexing)
content.transformer.img2ocr2txt.priority=30
content.transformer.img2ocr2txt.extensions.tiff.txt.priority=30
content.transformer.img2ocr2txt.extensions.tiff.txt.supported=true
# PNG -> TXT (Indexing)
content.transformer.img2ocr2txt.extensions.png.txt.priority=30
content.transformer.img2ocr2txt.extensions.png.txt.supported=true
# JPG -> TXT (Indexing)
content.transformer.img2ocr2txt.extensions.jpg.txt.priority=30
content.transformer.img2ocr2txt.extensions.jpg.txt.supported=true

