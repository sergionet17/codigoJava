		<EAModel.scenario>
			<EAScenario name="Basic Path" type="Basic Path" weight="0,00" subject="EAID_245D1229_8514_4099_BF38_FE361E84DF98" xmi.id="EAID_9C3AB1D0_F7FF_4df5_A046_AA0F3E52DE05">
				<EAScenarioContent>
					<path>
						<step name="El usuario ingresa a la opción de consulta de documentos y escribe la opción de consulta entre las siguiente: Identificación del documento&#xA;* Metadata indexada&#xA;* Clasificación por el nombre de sus nodos de clasificación&#xA;* Contenido textual (RF425) (Full Text search sosterior a un proceso de extracción de texto)&#xA;" guid="{45E93B16-E0B1-4b43-AE8E-737563C889B4}" level="1" uses="" useslist="" result="" state="" trigger="1" link=""/>
						<step name="El sistema realiza la consulta dependiendo de la opción seleccionada y entrega los documentos producto del resultado de la búsqueda (Los documentos pueden encontrarse en diferentes expedientes, o no encontrarse clasificados)." guid="{538201B2-936C-4b41-B1DF-BD611E1F0A05}" level="2" uses="" useslist="" result="Listar documentos producto de búsqueda por metadata.&#xA;Listar documentos producto de búsqueda por clasificación.&#xA;Listar documentos producto de búsqueda por contenido textual.&#xA;" state="" trigger="0" link=""/>
						<context/>
					</path>
				</EAScenarioContent>
			</EAScenario>
			<EAScenario name="Basic Path" type="Basic Path" weight="0,00" subject="EAID_270A95A6_47DD_4764_AE6A_2096BE157A07" xmi.id="EAID_29780FC5_28CB_4b59_9DE8_025ABF00FFDB">
				<EAScenarioContent>
					<path>
						<step name="El usuario selecciona un nodo de la clasificación y consulta su contenido." guid="{EB779E1A-3259-49cb-85A7-A47ADC8C0BA8}" level="1" uses="" useslist="" result="Listar los nodos folder fijo de un nodo folder.&#xA;Listar los documentos que pertenecer  a un nodo folder.&#xA;" state="" trigger="1" link=""/>
						<step name="El usuario selecciona un nodo hijo y puede realizar de nuevo la operación O1." guid="{5356AD5F-300F-439a-B97B-349252B83DEC}" level="2" uses="" useslist="" result="" state="" trigger="0" link=""/>
						<context/>
					</path>
				</EAScenarioContent>
			</EAScenario>
			<EAScenario name="Basic Path" type="Basic Path" weight="0,00" subject="EAID_89523091_4B2A_4524_91E1_24E553D229CC" xmi.id="EAID_C472870C_2D4E_47e7_AD07_92EF76E207EC">
				<EAScenarioContent>
					<path>
						<step name="El sistema realiza el proceso de extracción de texto de un soporte digital siempre que el documento tenga marcado dentro de su metadata que debe ser indexado y la cantidad de páginas de las cuales se desea extraer." guid="{2FD0628C-16B0-4daf-B1CD-AECFDC68E19B}" level="1" uses="" useslist="" result="Extraer texto de documentos con texto digital integrado.&#xA;Extraer texto de documentos imagen mediante proceso de OCR.&#xA;" state="" trigger="1" link=""/>
						<step name="Indexar el contenido extraído del documento. " guid="{EABF6143-3C89-4bab-B369-6D0DD5D49862}" level="2" uses="" useslist="" result="Verificar el registro de indexado del contenido del documento" state="" trigger="0" link=""/>
						<context/>
					</path>
				</EAScenarioContent>
			</EAScenario>
			<EAScenario name="Basic Path" type="Basic Path" weight="0,00" subject="EAID_CBC458E9_0F79_4e7e_B833_E4458B03A30A" xmi.id="EAID_403263A3_2397_4188_A320_72D7D383F3D9">
				<EAScenarioContent>
					<path>
						<step name="El sistema expone una interfaz de servicios por medio de la cual se ofrecer la siguiente funcionalidad:&#xA;&#xA;a. Crear nuevos documentos o anexos&#xA;b. Clasificar los documentos&#xA;c. Reasignación.&#xA;d. Crear accesos directo a documentos.&#xA;e. Adicionar los documentos en un expediente o subexpediente&#xA;f. Consultar documentos&#xA;g. Definir ACL del documento.&#xA;h. Firmar digitalmente documentos.&#xA;" guid="{E1C311C4-3C49-4e4c-A76A-5AA9657A9359}" level="1" uses="" useslist="" result="Consumir el servicio Crear nuevos documentos o anexos y verificar la acción.&#xA;Consumir el servicio clasificar documentos y verificar la acción.&#xA;Consumir el servicio reasignación y verificar la acción.&#xA;Consumir el servicio crear acceso directo a documento y verificar la acción.&#xA;Consumir el servicio adicionar los documentos en un expediente o sub-expediente y verificar la acción.&#xA;Consumir el servicio consultar documentos y verificar la acción.&#xA;Consumir el servicio definir ACL y verificar la acción.&#xA;Consumir el servicio firmar digitalmente y verificar la acción.&#xA;" state="" trigger="1" link=""/>
						<context/>
					</path>
				</EAScenarioContent>
			</EAScenario>
			<EAScenario name="Basic Path" type="Basic Path" weight="0,00" subject="EAID_CE1F371D_8592_4528_AFDC_2C2FCA981CEE" xmi.id="EAID_0DF301AE_0A14_42c0_9530_D358FA8D1E3B">
				<EAScenarioContent>
					<path>
						<step name="El usuario selecciona el nodo (Documento o expediente o nodo de clasificación) sobre el cual va a definir su lista de control de acceso ACL." guid="{C195D63D-EB6D-4384-BF37-42D37CE253A8}" level="1" uses="" useslist="" result="" state="" trigger="1" link=""/>
						<step name="El usuario selecciona el usuario o el grupo de usuarios o el rol sobre el cual se aplica la lista de control de acceso ACL." guid="{1BFDF9F8-CE4C-455a-A041-6025752ABBAD}" level="2" uses="" useslist="" result="" state="" trigger="0" link=""/>
						<step name="El usuario indica los permisos de la entidad seleccionada sobre el nodo seleccionado indicándolo dentro de la siguiente lista de chequeo:&#xA;Lectura&#xA;Escritura&#xA;Eliminación&#xA;" guid="{0E4D5F0A-5282-45fc-9DB2-EEC750BCBEEB}" level="3" uses="" useslist="" result="Verificar permiso de escritura.&#xA;Verificar permiso de lectura.&#xA;Verificar permiso de eliminación.&#xA;" state="" trigger="1" link=""/>
