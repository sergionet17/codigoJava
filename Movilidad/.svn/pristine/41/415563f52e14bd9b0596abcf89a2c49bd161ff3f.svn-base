package co.gov.movilidadbogota.sipa.sso.conf;

import org.springframework.security.core.AuthenticationException;

/**
 * Excepcion cuando el usuario tiene fecha de caducidad expirada
 *
 * @author lorenzo.pinango
 */
public class PasswdExpiredException extends AuthenticationException {

    public PasswdExpiredException(String msg, Throwable t) {
        super(msg, t);
    }

    public PasswdExpiredException(String msg) {
        super(msg);
    }
}
